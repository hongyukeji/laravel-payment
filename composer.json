{
    "name": "hongyukeji/laravel-payment",
    "description": "Laravel payment.",
    "authors": [
        {
            "name": "shadow",
            "email": "admin@hongyuvip.com"
        }
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "~5.0 || ~6.0",
        "omnipay/paypal": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "~6.5 || ~7.0"
    },
    "autoload": {
        "psr-4": {
            "Hongyukeji\\LaravelPayment\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hongyukeji\\LaravelPayment\\Providers\\PaymentServiceProvider"
            ],
            "alias": {
                "Payment": "Hongyukeji\\LaravelPayment\\Facades\\Payment"
            }
        }
    },
    "minimum-stability":"dev"
}
